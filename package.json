{
  "name": "template-electron",
  "productName": "template-electron",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "start:debug": "electron-forge start --inspect-electron",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix ."
  },
  "keywords": [],
  "author": {
    "name": "Luis Fontes",
    "email": "luisfontes@protonmail.com"
  },
  "license": "MIT",
  "build": {
    "appId": "com.thesecurityvault",
    "productName": "template-electron",
    "copyright": "Copyright Â© year ${author}",
    "fileAssociations": [
      {
        "ext": [
          "test"
        ],
        "name": "Test files :D",
        "role": "Editor"
      }
    ]
  },
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "6.0.0-beta.61",
    "@types/node": "^16.11.12",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "css-loader": "^6.0.0",
    "electron": "16.0.4",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.2.1",
    "@mui/lab": "^5.0.0-alpha.59",
    "@mui/material": "^5.2.3",
    "autotype": "file:src/native_modules/autotype",
    "bindings": "^1.5.0",
    "electron-rebuild": "^3.2.5",
    "node-gyp": "^8.4.1"
  }
}